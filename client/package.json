{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "@cloudinary/react": "^1.4.1",
    "@cloudinary/url-gen": "^1.8.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^13.1.0",
    "@testing-library/user-event": "^14.1.0",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.1.0",
    "jquery": "^3.6.0",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-https-redirect": "^1.1.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "concurrently \"npm run watch:css\" \"react-scripts start\"",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "postcss ./src/assets/tailwind/input.css -o ./src/assets/tailwind/main.css",
    "watch:css": "postcss ./src/assets/tailwind/input.css -o ./src/assets/tailwind/main.css --watch"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
